{
  "name": "react-vite-eslint-prettier",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "serve dist/client",
    "lint": "run-s lint:*",
